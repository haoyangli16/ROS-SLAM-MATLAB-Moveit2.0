      <inertia
        ixx="10833.3"
        ixy="0.0"
        ixz="0.0"
        iyy="10833.3"
        iyz="0.0"
        izz="20000" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://pudong/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
